#include <iostream>
using namespace std;

int main()
{
    int x, y, z;
    int reward = 0;

    cin >> x >> y >> z;

    if (x == y && x == z && y == z)
    {
        reward = 10000 + x * 1000;
        cout << reward << endl;
    }
    else if (x != y && y != z && x != z)
    {
        if (x > y && x > z)
            reward = x * 100;
        else if (y > x && y > z)
            reward = y * 100;
        else
            reward = z * 100;

        cout << reward << endl;
    }
    else
    {
        if (x == y)
            reward = 1000 + x * 100;
        else if (y == z)
            reward = 1000 + y * 100;
        else if (x == z)
            reward = 1000 + z * 100;

        cout << reward << endl;
    }
    return 0;
}
